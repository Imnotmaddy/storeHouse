<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity Product.
    -->
    <changeSet id="20190530162440-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="act_id"
                                 baseTableName="product"
                                 constraintName="fk_product_act_id"
                                 referencedColumnNames="id"
                                 referencedTableName="act"/>

        <addForeignKeyConstraint baseColumnNames="storage_room_id"
                                 baseTableName="product"
                                 constraintName="fk_product_storage_room_id"
                                 referencedColumnNames="id"
                                 referencedTableName="storage_room"/>

        <addForeignKeyConstraint baseColumnNames="ttn_id"
                                 baseTableName="product"
                                 constraintName="fk_product_ttn_id"
                                 referencedColumnNames="id"
                                 referencedTableName="ttn"/>

    </changeSet>
</databaseChangeLog>
